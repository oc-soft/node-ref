{
  "name": "ref",
  "description": "Turn Buffer instances into \"pointers\"",
  "keywords": [
    "native",
    "buffer",
    "extensions",
    "c++",
    "pointer",
    "reference",
    "dereference",
    "type",
    "int",
    "long",
    "float",
    "double",
    "byte",
    "64"
  ],
  "version": "2.0.0",
  "license": "MIT",
  "author": "Nathan Rajlich <nathan@tootallnate.net> (http://tootallnate.net)",
  "repository": {
    "type": "git",
    "url": "git://github.com/TooTallNate/ref.git"
  },
  "main": "./lib/ref.js",
  "types": "./lib/ref.d.ts",
  "scripts": {
    "docs": "node docs/compile",
    "type-check": "tsc --noEmit lib/ref.d.ts",
    "test": "node --trace-deprecation --expose-gc node_modules/mocha/lib/cli/cli.js --reporter spec --use_strict",
    "test-ts": "ts-node --project test-ts/tsconfig.json test-ts/ref-t.ts"
  },
  "dependencies": {
    "bindings": "latest",
    "debug": "latest",
    "nan": "latest"
  },
  "devDependencies": {
    "@electron/rebuild": "latest",
    "@types/mocha": "latest",
    "@types/node": "latest",
    "dox": "latest",
    "highlight.js": "latest",
    "jade": "latest",
    "marked": "latest",
    "mocha": "latest",
    "ts-node": "latest",
    "typescript": "latest",
    "weak": "oc-soft/node-weak#master"
  }
}
